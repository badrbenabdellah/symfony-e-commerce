{% extends 'base.html.twig' %}

{% block body %}
    <div class="container"><br>

        <table class="table">
            <tr>
                <th>Id</th>
                <th>Quantity</th>
                <th>Date Added</th>
            </tr>

        {% for productAdded in productsAdded %}
            <tr>
                <td>{{ productAdded.id }}</td>
                <td>{{ productAdded.quantity }}</td>
                <td>{{ productAdded.createdAt|date('d/m/Y H:i:s') }}</td>
            </tr>
        {% endfor %}
        </table>
    </div>
{% endblock %}
